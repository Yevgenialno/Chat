@page
@model chat.Pages.DialogModel
@{
    foreach (var message in Model.messages)
    {
        <P>@message.Content</P>
    }
}
<form method="post">
    <input type="text", asp-for="NewMessage.Content">
    <button class="btn btn-primary">Send</button>
</form>
<p>Your Tag: @Model.userLoginned?.Tag</p>
